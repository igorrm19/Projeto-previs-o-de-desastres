{
  "include": [
    "src"
  ],
  "exclude": [
    "src/config/developmet.js",
    "src/config/production.js",
    "src/config/teste.js",
    "src/features/error/error.js",
    "src/features/feature.js"
  ],
  "compilerOptions": {
    "outDir": "dist",
    "jsx": "react-jsx",
    "module": "commonjs",
    "target": "es2016",
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true /* Allow JavaScript files to be compiled. */
  }
}